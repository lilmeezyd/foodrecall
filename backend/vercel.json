{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js",
        "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
      }
    ],
    "crons": [
      {
        "path": "/api/checkFdaApi",
        "schedule": "55 14 * * *"
      }
    ]
  }